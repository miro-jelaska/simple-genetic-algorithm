geneticAlgorithm = {
    transformMetaPopulationIntoMatingPool: function(metaPopulation){
    let matingPool = [];
    for(let metaMemberIndex = 0; metaMemberIndex < metaPopulation.length; metaMemberIndex++){
        let metaMember = metaPopulation[metaMemberIndex];
        for(let i = 0; i < Math.round(metaMember.fitnessRate * 100); i++){         
            matingPool.push(metaMember.genome)
        }
    }
    return matingPool;
}
}

module.exports = geneticAlgorithm